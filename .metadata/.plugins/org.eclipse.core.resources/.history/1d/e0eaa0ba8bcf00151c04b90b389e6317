import org.teachingextensions.logo.Tortoise;

public class obedientturtle {
public static void main(String[] args) {
	Tortoise.show();
	Tortoise.setSpeed(7);
	
	Tortoise.turn(30);
	Tortoise.move(70);
	Tortoise.turn(90);
	Tortoise.move(70);
	Tortoise.turn(30);

	//drawSquare();
}

